package com.Test.qa.testcases;

	public class parent {
	    int x;

	    parent() {
	        x = 5;
	    }

	}

	public class child extends parent {
	int y;

	child() {
	    super();
	    y = 10;
	}

	public static void main(String[] args) {

	    parent p = new child();
	    System.out.println(p.y);//can't access variable y
	}